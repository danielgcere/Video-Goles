{
    "type": "APL",
    "version": "1.1",
    "import": [
        {
            "name": "[APL_NAME]",
            "version": "1.0.0",
            "source": "https://[APL_SERVER]/[APL_VERSION]/templates/[APL_NAME].json"
        }
    ],
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "Inflate",
                "payload": "${payload}"
            }
        ]
    }
}